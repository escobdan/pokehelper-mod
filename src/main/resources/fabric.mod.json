{
	"schemaVersion": 1,
	"id": "pokehelper",
	"version": "${version}",
	"name": "pokehelper",
	"description": "This is a cobblemon helper mod that will update custom website when battling pokemon.",
	"authors": [
		"Daniel Escobar"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MPL_2.0",
	"icon": "assets/pokehelper/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.escobdan.pokehelper.Pokehelper"
		],
		"client": [
			"net.escobdan.pokehelper.PokehelperClient"
		],
		"fabric-datagen": [
			"net.escobdan.pokehelper.PokehelperDataGenerator"
		]
	},
	"mixins": [
		"pokehelper.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}